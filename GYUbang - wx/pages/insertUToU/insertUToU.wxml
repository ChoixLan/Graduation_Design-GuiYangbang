<view>
    <van-cell-group>
        <van-field model:value="{{ addressQ }}" size="large" clickable clearable label="取件地址：" placeholder="请输入取件地址"/>
    </van-cell-group>
    <van-cell-group>
        <van-field model:value="{{ addressS }}" size="large" clickable clearable label="送件地址：" placeholder="请输入送件地址"/>
    </van-cell-group>
    <van-cell-group>
        <van-field model:value="{{ password }}" size="large" clickable clearable label="取件密码：" placeholder="请输入取件密码"/>
    </van-cell-group>
    <van-cell-group>
        <van-field model:value="{{ money }}" size="large" clickable clearable label="悬赏金额：" placeholder="请输入悬赏金额"/>
    </van-cell-group>

    <!-- 弹窗 -->
    <van-dialog id="van-dialog" />

    <view style="text-align: center; margin-top: 200rpx;">
        <button bindtap="insertUToU" type="primary" style="width: 50%;">发起维修申请</button>
    </view>
</view>